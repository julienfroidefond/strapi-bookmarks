{
  "name": "Strapi bookmarks",
  "version": "3.0.2",
  "manifest_version": 2,
  "description": "This extension synchronize bookmarks from a Strapi server. cf https://github.com/julienfroidefond/bookmarks-cms.",
  "homepage_url": "http://cdiscount.com",
  "icons": {
    "16": "strapi-bookmarks-16.png",
    "48": "strapi-bookmarks-48.png",
    "128": "strapi-bookmarks-128.png"
  },
  "browser_action": {
    "default_icon": "strapi-bookmarks-128.png",
    "default_title": "Strapi bookmarks",
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "permissions": ["bookmarks", "storage"],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "background": {
    "page": "background.html"
  }
}
